{
  "name": "discord",
  "version": "1.0.0",
  "description": "discord clone",
  "main": "index.js",
  "scripts": {
    "import": "postcss assets/styles/main.css --use postcss-import -o assets/css/main.import.css",
    "prefix": "postcss assets/css/main.import.css --use autoprefixer -o assets/css/main.pre.css",
    "minify": "postcss assets/css/main.pre.css --use cssnano -o assets/css/main.min.css",
    "dev": "npm-run-all import prefix minify"
  },
  "author": "Hamza",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.3.1",
    "cssnano": "^5.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2"
  }
}
